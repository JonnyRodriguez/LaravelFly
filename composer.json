{
  "name": "scil/laravel-fly",
  "description": "make laravel faster by using swoole extension.",
  "type": "library",
  "keywords": [
    "swoole",
    "laravel"
  ],
  "homepage": "http://www.github.com/scil/laravel_fly",
  "license": "MIT",
  "authors": [
    {
      "name": "scil"
    }
  ],
  "require": {
    "laravel/framework": "5.5.*",
    "ext-swoole": "^1.9.23 || >=2.1.0"
  },
  "require-dev": {
    "eaglewu/swoole-ide-helper": "dev-master",
    "nunomaduro/collision": "^1.1"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "LaravelFly\\": "src/LaravelFly/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFly\\Providers\\PublishServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
